      "type": "string"
    },
    "details": {
      "type": "string"
    },
    "vendor": {
      "type": "string"
    },
    "purchasedAt": {
      "type": "date"
    },
    "usingBy": {
      "type": "relation",
      "relation": "oneToOne",
